<p id="notice"><%= notice %></p>

<h1>Listing Paper Assignments</h1>

<table>
  <thead>
    <tr>
      <th>Teacher</th>
      <th>Paper</th>
      <th>Batch</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @paper_assignments.each do |paper_assignment| %>
      <tr>
        <td><%= paper_assignment.teacher %></td>
        <td><%= paper_assignment.paper %></td>
        <td><%= paper_assignment.batch %></td>
        <td><%= link_to 'Show', paper_assignment %></td>
        <td><%= link_to 'Edit', edit_paper_assignment_path(paper_assignment) %></td>
        <td><%= link_to 'Destroy', paper_assignment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Paper assignment', new_paper_assignment_path %>
