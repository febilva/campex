<p id="notice"><%= notice %></p>

<h1>Listing Term Structure Entries</h1>

<table>
  <thead>
    <tr>
      <th>Term structure</th>
      <th>Name</th>
      <th>Code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @term_structure_entries.each do |term_structure_entry| %>
      <tr>
        <td><%= term_structure_entry.term_structure %></td>
        <td><%= term_structure_entry.name %></td>
        <td><%= term_structure_entry.code %></td>
        <td><%= link_to 'Show', term_structure_entry %></td>
        <td><%= link_to 'Edit', edit_term_structure_entry_path(term_structure_entry) %></td>
        <td><%= link_to 'Destroy', term_structure_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Term structure entry', new_term_structure_entry_path %>
