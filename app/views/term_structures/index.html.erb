<p id="notice"><%= notice %></p>

<h1>Listing Term Structures</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @term_structures.each do |term_structure| %>
      <tr>
        <td><%= term_structure.name %></td>
        <td><%= link_to 'Show', term_structure %></td>
        <td><%= link_to 'Edit', edit_term_structure_path(term_structure) %></td>
        <td><%= link_to 'Destroy', term_structure, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Term structure', new_term_structure_path %>
