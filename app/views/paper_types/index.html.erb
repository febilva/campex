<p id="notice"><%= notice %></p>

<h1>Listing Paper Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @paper_types.each do |paper_type| %>
      <tr>
        <td><%= paper_type.name %></td>
        <td><%= paper_type.code %></td>
        <td><%= link_to 'Show', paper_type %></td>
        <td><%= link_to 'Edit', edit_paper_type_path(paper_type) %></td>
        <td><%= link_to 'Destroy', paper_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Paper type', new_paper_type_path %>
