<div class="examination-report">
  <div class="container">
    <div class="university">
      <h3><%= @term.name %> Internal Marks</h3>
      <h3><span><%= "#{@batch.course.full_name} ( #{@batch.name} )" %></span></h3>
    </div>

  <div class="table-team">
    <table class="ui celled striped table">
      <thead>
        <tr>
          <th>Code</th>
          <th>Name of Course</th>
        </tr>
      </thead>
      <tbody>
        <% @papers.each do |paper| %>
          <tr>
            <td class="collapsing"><%= paper[:code] %></td>
            <td><%= paper[:name] %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
 </div>
</div>

<%= render 'form' %>

<%= link_to 'Back', batch_term_dates_path(@batch) %>
